{
    "events": {
        "module": "./events.js"
    },
    "stations": {
        "module": "./stations.js",
        "default": {
            "module": "./default-stations.js"
        }
    },
    "player": {
        "module": "./mplayer.js",
        "stations": "-> stations",
        "board": {
            "channels": 1
        },
        "events": "-> events",
        "checkNetwork": "=> wifi.checkNetwork"
    },
    "i2c": {
        "module": "./i2c.js",
        "player": "-> player",
        "drivers": {
            "0x40": {
                "module": "./drv-encoder.js",
                "onRotate": "changeVolume",
                "onPush": "toggleAperture"
            },
            "0x41": {
                "module": "./drv-encoder.js",
                "onRotate": "changeBalance",
                "onPush": "resetBalance"
            },
            "0x42": {
                "module": "./drv-encoder.js",
                "onRotate": "changeTreble",
                "onPush": "resetTreble"
            },
            "0x43": {
                "module": "./drv-encoder.js",
                "onRotate": "changeBass",
                "onPush": "resetBass"
            },
            "0x44": {
                "module": "./drv-encoder.js",
                "onRotate": "changeAperture",
                "onPush": "resetAperture"
            },
            "0x45": {
                "module": "./drv-encoder.js",
                "onRotate": "changeTuning",
                "onPush": "resetTuning"
            },
            "0x46": {
                "module": "./drv-encoder.js",
                "onRotate": "changeBand",
                "onPush": "resetBand"
            },
            "0x49": {
                "module": "./drv-buttons.js"
            }
        }
    },
    "wifi": {
        "module": "./wifi.js",
        "player": "-> player"
    },
    "webglue": {
        "module": "@device.farm/webglue",
        "httpPort": "-> $.HTTP_PORT || 8080",
        "modules": [
            {
                "module": "./webui-server.js",
                "stations": "-> stations",
                "player": "-> player",
                "events": "-> events"
            }
        ]
    }
}